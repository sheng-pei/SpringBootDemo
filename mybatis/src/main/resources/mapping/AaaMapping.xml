<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.mybatis.mapper.AaaMapper">
    <insert id="batchInsert">
        insert into aaa (id, name, is_deleted) values
                                                   <foreach collection="aaas" item="aaa" open="" close="" separator=",">
                                                       (#{aaa.id}, #{aaa.name}, #{aaa.deleted})
                                                   </foreach>

    </insert>
    <update id="uuuu">
        update aaa set name = #{name} where id = #{id}
    </update>
</mapper>